{
    "version": 2,
    "builds": [
      {
        "src": "mynew_django/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime":"python3.8",
            "buildCommand": "bash vercel-build-steps.sh"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "mynew_django/wsgi.py"
      }
    ]
}